<aura:component implements="lightning:isUrlAddressable,forceCommunity:themeLayout" access="global" >
	<aura:attribute name="menu" type="Aura.Component[]" required="false" />
    <aura:attribute name="header" type="Aura.Component[]" required="false" />
    <aura:attribute name="searchResult" type="Aura.Component[]" required="false" />
    <aura:attribute name="body" type="Aura.Component[]" required="false" />
    
    <div class="slds-grid slds-wrap">
        <div class="slds-col slds-size_1-of-1 slds-medium-size_3-of-12 slds-large-size_2-of-12">
            <span>{!v.menu}</span>
        </div>
        <div class="slds-col slds-size_1-of-1 slds-medium-size_9-of-12 slds-large-size_10-of-12">
            <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-1 slds-large-size_1-of-1">
                <span>{!v.header}</span>
            </div>
            <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_5-of-12">
                <span>{!v.searchResult}</span>
            </div>
            <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_7-of-12">
                <span>{!v.body}</span>
            </div>
        </div>
    </div>
</aura:component>